    // ],
    // "python.defaultInterpreterPath": "C:\\Program Files\\Python310\\python.exe",
    "python.analysis.autoImportCompletions": true,
    "notebook.output.textLineLimit": 100,
    "C_Cpp.vcFormat.indent.lambdaBracesWhenParameter": false,
    "C_Cpp.vcFormat.newLine.beforeOpenBrace.function": "newLine",
    "C_Cpp.errorSquiggles": "enabled",
    "[python]": {
        "editor.formatOnType": true
    },
    "files.autoSaveDelay": 1000,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "variable.parameter.cpp",
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    },
    "github.gitProtocol": "ssh",
    "git.openRepositoryInParentFolders": "prompt",
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "workbench.colorCustomizations": {
        // "editor.lineHighlightBackground": "#00000000",
    },
    "editor.lineNumbers": "relative",
    "window.zoomLevel": 1,
    "editor.guides.indentation": false,
    "editor.occurrencesHighlight": "off",
    "editor.renderLineHighlight": "none",
    "editor.matchBrackets": "near",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingQuotes": "never",
    "editor.cursorBlinking": "solid",
    "editor.acceptSuggestionOnEnter": "off",
    "editor.wordBasedSuggestions": "off",
    "editor.semanticHighlighting.enabled": true,
    "editor.detectIndentation": true,
    "redhat.telemetry.enabled": false,
    "editor.minimap.enabled": false,
    "editor.hideCursorInOverviewRuler": true,
    "editor.scrollbar.horizontal": "hidden",
    "editor.scrollbar.vertical": "hidden",
    "editor.scrollbar.verticalScrollbarSize": 0,
    "editor.scrollbar.horizontalScrollbarSize": 0,
    "workbench.colorTheme": "Kanagawa Dragon",
    "editor.suggest.insertMode": "insert",
    "editor.stickyScroll.enabled": false,
    "task.quickOpen.showAll": true,
    "search.quickOpen.includeHistory": false,
    "editor.cursorSurroundingLines": 8,

    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.surround": true,
    "vim.history": 100,
    "vim.wrapscan": false,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": false,
    "vim.insertModeKeyBindings": [
        {
            "before": ["<C-c>"],
            "after": ["<Esc>"]
        },
    ],
    "vim.visualModeKeyBindingsNonRecursive": [
        // {
        //     "before": ["<C-n>"],
        //     "after": ["g", "b"],
        // },
        {
            "before": ["<C-c>"],
            "after": ["<Esc>"]
        },
        {
            "before": ["<C-n>"],
            "commands": ["editor.action.addSelectionToNextFindMatch"],
        },
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": ["<leader>", "d"],
            "after": ["d", "d"]
        },
        {
            "before": ["<C-J>"],
            "after": ["<C-up>"]
        },
        {
            "before": ["<C-c>"],
            "after": ["<Esc>"]
        },
        {
            "before": ["<C-l>"],
            "commands": [":nohl"],
            "silent": true,
        },
        {
            "before": ["g", "d"],
            "command": "editor.action.goToDeclaration",
        },
        {
            "before": ["<C-n>"],
            "commands": ["editor.action.addSelectionToNextFindMatch"],
        },
        {
            "before": ["K"],
            "commands": ["editor.action.showDefinitionPreviewHover"]
        },
        // {
        //     "before": [" ", " "],
        //     "commands": ["workbench.action.quickOpen"],
        //     "silent": true,
        // }
        // {
        //     "before": ["A-k"],
        //     "when": "editorTextFocus",
        //     "commands": ["editor.action.insertCursorAbove"],
        // },
        // {
        //     "before": ["<A-j>"],
        //     "when": "editorTextFocus",
        //     "commands": ["editor.action.insertCursorBelow"],
        // },
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<tab>": false,
        "<C-n>": true,
        "<C-p>": false,
        "<C-w>": true,
        "<C-k>": true,
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
}
